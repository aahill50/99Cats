<h1>These are all the Cats for sale</h1>

<table>
  <tr>
     <td> Name        </td>
     <td> Birth Date  </td>
     <td> Color       </td>
     <td> Gender      </td>
     <td> Description </td>
  </tr>
<% @cats.each do |cat| %>
  <tr>
    <td><a href=<%= cat_url(cat) %>><%= cat.name %></a></td>
    <td> <%=cat.birth_date%> </td>
    <td> <%=cat.color%> </td>
    <td> <%=cat.sex%> </td>
    <td> <%=cat.description%> </td>
    <td> <a href=<%= edit_cat_url(cat) %>>Edit Cat the coooool way</a> </td>
  </tr>
  <% end %>
</table>

<p> <a href=<%= new_cat_url %>>Make a cat!!! </a> </p>
<p> <a href=<%= new_user_url %>> Sign up!!! </a> </p>
<p> <a href=<%= new_session_url %>> Sign in!!! </a></p>
