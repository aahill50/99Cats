<h1>These are all the Cats for sale</h1>

<table>
  <tr>
     <td> Name        </td>
     <td> Birth Date  </td>
     <td> Color       </td>
     <td> Gender      </td>
     <td> Description </td>
  </tr>
<% @cats.each do |cat| %>
  <tr>
    <td><a href=<%= cat_url(cat) %>><%= cat.name %></a></td>
    <td> <%=cat.birth_date%> </td>
    <td> <%=cat.color%> </td>
    <td> <%=cat.sex%> </td>
    <td> <%=cat.description%> </td>
  </tr>
  <% end %>
</table>
